n = int(input())
m = int(input())

matrix = [[input() for _ in range(m)] for _ in range(n)]
    
for i in range(n):
    print(*matrix[i])
print()
for j in range(m):
    for i in range(n):
        print(matrix[i][j], end=" ")
    print()

#вычисление следа матрицы

n = int(input())

matrix = [input().split() for _ in range(n)]

s = 0
for i in range(n):
    s += int(matrix[i][i])

print(s)
